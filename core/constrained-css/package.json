{
  "name": "constrained-css",
  "main": "src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsc --noEmit --project tsconfig.json",
    "test": "pnpm test:unit run && pnpm test:types --run",
    "test:watch": "concurrently -n unit,types \"pnpm test:unit\" \"pnpm test:types\"",
    "test:unit": "vitest",
    "test:types": "vitest typecheck"
  },
  "dependencies": {
    "@yas/result": "workspace:*"
  },
  "devDependencies": {
    "@yas/tsconfig": "workspace:*",
    "@yas/test": "workspace:*",
    "csstype": "3.1.3",
    "concurrently": "^8.2.1"
  }
}
