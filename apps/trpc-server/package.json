{
  "name": "@yas/trpc-server",
  "version": "0.0.0",
  "private": true,
  "types": "./src/package.ts",
  "exports": {
    ".": {
      "import": "./src/package.ts",
      "require": "./src/package.ts"
    }
  },
  "scripts": {
    "build": "pnpm typecheck && yas-env tsup",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "dev": "yas-env tsx watch src/entrypoint.ts --clear-screen=false",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@trpc/server": "11.0.0-next-beta.316",
    "@yas/validate": "workspace:*",
    "express": "4.18.3",
    "superjson": "1.13.3"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/morgan": "1.9.9",
    "@yas/build": "workspace:*",
    "@yas/env": "workspace:*",
    "@yas/test": "workspace:*",
    "@yas/tsconfig": "workspace:*",
    "cors": "2.8.5",
    "morgan": "1.10.0"
  }
}
