{
  "name": "@yas/api",
  "version": "0.0.0",
  "private": true,
  "main": "./sdk/index.ts",
  "types": "./sdk/index.ts",
  "exports": {
    ".": {
      "import": "./sdk/index.ts",
      "require": "./sdk/index.ts"
    }
  },
  "scripts": {
    "build": "pnpm typecheck && yas-env tsup",
