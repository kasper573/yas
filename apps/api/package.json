    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.5.8",
    "@yas/build-tools": "workspace:*",
    "@yas/env": "workspace:*",
    "@yas/jest": "workspace:*",
    "@yas/tsconfig": "workspace:*",
    "cors": "^2.8.5",
