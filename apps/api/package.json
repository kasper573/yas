  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/morgan": "^1.9.4",
    "@types/node": "^20.5.8",
    "@yas/build": "workspace:*",
    "@yas/env": "workspace:*",
    "@yas/test": "workspace:*",
    "@yas/tsconfig": "workspace:*",
    "cors": "^2.8.5",
    "morgan": "^1.10.0",
    "tsup": "^7.2.0",
    "ts-node-dev": "^2.0.0"
  }
