{
  "name": "@yas/api",
  "version": "0.0.0",
  "private": true,
  "types": "./src/package.ts",
  "exports": {
    ".": {
      "import": "./src/package.ts",
      "require": "./src/package.ts"
    }
  },
  "scripts": {
    "build": "pnpm typecheck && yas-env tsup",
    "typecheck": "tsc --noEmit --project tsconfig.json",
    "dev": "yas-env tsnd src/entrypoint.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@trpc/server": "10.44.1",
    "@yas/result": "workspace:*",
    "@yas/validate": "workspace:*",
    "express": "4.18.2"
  },
  "devDependencies": {
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/morgan": "1.9.9",
    "@yas/build": "workspace:*",
    "@yas/env": "workspace:*",
    "@yas/test": "workspace:*",
    "@yas/tsconfig": "workspace:*",
    "cors": "2.8.5",
    "morgan": "1.10.0",
    "ts-node-dev": "2.0.0",
    "tsup": "8.0.1"
  }
}
